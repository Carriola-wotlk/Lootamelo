<Ui xmlns="http://www.blizzard.com/wow/ui/">

	<Frame name="Lootamelo_ConfigFrame" parent="Lootamelo_MainFrame">
		<Size x="512" y="512"/>
		<Anchors>
			<Anchor point="CENTER">
				<Offset x="75" y="0"/>
			</Anchor>
		</Anchors>
		<Frames>
			<Button name="$parentDropDownButton" inherits="UIDropDownMenuTemplate">
				<Size x="150" y="30"/>
				<Anchors>
					<Anchor point="TOP">
						<Offset x="-7" y="-100"/>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnLoad>
						UIDropDownMenu_Initialize(self, Lootamelo_ConfigFrameInitDropDown)
						UIDropDownMenu_SetText(self, "Raids")
					</OnLoad>
				</Scripts>
			</Button>
		</Frames>
	</Frame>

</Ui>
